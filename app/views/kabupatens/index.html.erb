<p id="notice"><%= notice %></p>

<h1>Kabupatens</h1>

<table>
  <thead>
    <tr>
      <th>Nama</th>
      <th>Provinsi</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @kabupatens.each do |kabupaten| %>
      <tr>
        <td><%= kabupaten.nama %></td>
        <td><%= kabupaten.provinsi %></td>
        <td><%= link_to 'Show', kabupaten %></td>
        <td><%= link_to 'Edit', edit_kabupaten_path(kabupaten) %></td>
        <td><%= link_to 'Destroy', kabupaten, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Kabupaten', new_kabupaten_path %>
